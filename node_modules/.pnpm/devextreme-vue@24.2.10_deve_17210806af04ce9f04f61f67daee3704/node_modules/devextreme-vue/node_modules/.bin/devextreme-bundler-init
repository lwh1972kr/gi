#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/dev/vue/re-mes2/node_modules/.pnpm/devextreme@24.2.10/node_modules/devextreme/bin/node_modules:/mnt/d/dev/vue/re-mes2/node_modules/.pnpm/devextreme@24.2.10/node_modules/devextreme/node_modules:/mnt/d/dev/vue/re-mes2/node_modules/.pnpm/devextreme@24.2.10/node_modules:/mnt/d/dev/vue/re-mes2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/dev/vue/re-mes2/node_modules/.pnpm/devextreme@24.2.10/node_modules/devextreme/bin/node_modules:/mnt/d/dev/vue/re-mes2/node_modules/.pnpm/devextreme@24.2.10/node_modules/devextreme/node_modules:/mnt/d/dev/vue/re-mes2/node_modules/.pnpm/devextreme@24.2.10/node_modules:/mnt/d/dev/vue/re-mes2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../devextreme@24.2.10/node_modules/devextreme/bin/bundler-init.js" "$@"
else
  exec node  "$basedir/../../../../../devextreme@24.2.10/node_modules/devextreme/bin/bundler-init.js" "$@"
fi
